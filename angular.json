{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "budgetkey-app-search": {
      "root": "..",
      "sourceRoot": "./",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": ".//index.html",
            "main": ".//main.ts",
            "tsConfig": ".//tsconfig.app.json",
            "assets": [
              "assets",
              "favicon.ico"
            ],
            "styles": [],
            "scripts": []
          },
          "configurations": {}
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "budgetkey-app-search:build"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "budgetkey-app-search:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": ".//undefined",
            "karmaConfig": "./karma.conf.js",
            "scripts": [],
            "styles": [],
            "assets": [
              "assets",
              "favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "budgetkey-app-search-e2e": {
      "root": "..",
      "sourceRoot": "../e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.config.js",
            "devServerTarget": "budgetkey-app-search:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "budgetkey-app-search",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}